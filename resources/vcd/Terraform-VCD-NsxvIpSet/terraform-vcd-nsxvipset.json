{
    "typeName": "Terraform::VCD::NsxvIpSet",
    "description": "Provides a vCloud Director IP set resource. An IP set is a group of IP addresses that you can add as\n  the source or destination in a firewall rule or in DHCP relay configuration.\n\n\nSupported in provider *v2.6+*",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Description": {
            "type": "string",
            "description": "An optional description for IP set."
        },
        "Id": {
            "type": "string"
        },
        "IpAddresses": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "A set of IP addresses, CIDRs and ranges as strings."
        },
        "IsInheritanceAllowed": {
            "type": "boolean"
        },
        "Name": {
            "type": "string",
            "description": "Unique IP set name."
        },
        "Org": {
            "type": "string",
            "description": "The name of organization to use, optional if defined at provider level. Useful when connected as sysadmin working across different organisations."
        },
        "Vdc": {
            "type": "string",
            "description": "The name of VDC to use, optional if defined at provider level."
        }
    },
    "additionalProperties": false,
    "required": [
        "IpAddresses",
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}