{
    "typeName": "Terraform::Triton::Fabric",
    "description": "The `triton_fabric` resource represents an fabric for a Triton account. The fabric is a logical set of interconnected switches.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Description": {
            "type": "string",
            "description": "Optional description of network."
        },
        "Fabric": {
            "type": "boolean"
        },
        "Gateway": {
            "type": "string",
            "description": "Optional gateway IP."
        },
        "Id": {
            "type": "string"
        },
        "InternetNat": {
            "type": "boolean",
            "description": "If a NAT zone is provisioned at Gateway IP address. Default is `true`."
        },
        "Name": {
            "type": "string",
            "description": "Network name."
        },
        "ProvisionEndIp": {
            "type": "string",
            "description": "Last assignable IP on the network."
        },
        "ProvisionStartIp": {
            "type": "string",
            "description": "First IP on the network that can be assigned."
        },
        "Public": {
            "type": "boolean"
        },
        "Resolvers": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            },
            "description": "Array of IP addresses for resolvers."
        },
        "Routes": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "MapKey": {
                        "type": "string"
                    },
                    "MapValue": {
                        "type": "string"
                    }
                },
                "required": [
                    "MapKey",
                    "MapValue"
                ]
            },
            "description": "Map of CIDR block to Gateway IP address."
        },
        "Subnet": {
            "type": "string",
            "description": "CIDR formatted string describing network."
        },
        "VlanId": {
            "type": "number",
            "description": "VLAN id the network is on. Number between 0-4095 indicating VLAN ID."
        }
    },
    "additionalProperties": false,
    "required": [
        "Name",
        "ProvisionEndIp",
        "ProvisionStartIp",
        "Subnet",
        "VlanId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Fabric",
        "/properties/Id",
        "/properties/Public"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}