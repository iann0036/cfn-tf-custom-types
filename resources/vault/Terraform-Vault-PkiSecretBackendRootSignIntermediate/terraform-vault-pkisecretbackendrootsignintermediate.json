{
    "typeName": "Terraform::Vault::PkiSecretBackendRootSignIntermediate",
    "description": "Creates an PKI certificate.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AltNames": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            },
            "description": "List of alternative names."
        },
        "Backend": {
            "type": "string",
            "description": "The PKI secret backend the resource belongs to."
        },
        "CaChain": {
            "type": "string"
        },
        "Certificate": {
            "type": "string"
        },
        "CommonName": {
            "type": "string",
            "description": "CN of intermediate to create."
        },
        "Country": {
            "type": "string",
            "description": "The country."
        },
        "Csr": {
            "type": "string",
            "description": "The CSR."
        },
        "ExcludeCnFromSans": {
            "type": "boolean",
            "description": "Flag to exclude CN from SANs."
        },
        "Format": {
            "type": "string",
            "description": "The format of data."
        },
        "Id": {
            "type": "string"
        },
        "IpSans": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            },
            "description": "List of alternative IPs."
        },
        "IssuingCa": {
            "type": "string"
        },
        "Locality": {
            "type": "string",
            "description": "The locality."
        },
        "MaxPathLength": {
            "type": "number",
            "description": "The maximum path length to encode in the generated certificate."
        },
        "Organization": {
            "type": "string",
            "description": "The organization."
        },
        "OtherSans": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            },
            "description": "List of other SANs."
        },
        "Ou": {
            "type": "string",
            "description": "The organization unit."
        },
        "PermittedDnsDomains": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            },
            "description": "List of domains for which certificates are allowed to be issued."
        },
        "PostalCode": {
            "type": "string",
            "description": "The postal code."
        },
        "Province": {
            "type": "string",
            "description": "The province."
        },
        "Serial": {
            "type": "string"
        },
        "StreetAddress": {
            "type": "string",
            "description": "The street address."
        },
        "Ttl": {
            "type": "string",
            "description": "Time to live."
        },
        "UriSans": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            },
            "description": "List of alternative URIs."
        },
        "UseCsrValues": {
            "type": "boolean",
            "description": "Preserve CSR values."
        }
    },
    "additionalProperties": false,
    "required": [
        "Backend",
        "CommonName",
        "Csr"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/CaChain",
        "/properties/Certificate",
        "/properties/Id",
        "/properties/IssuingCa",
        "/properties/Serial"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}