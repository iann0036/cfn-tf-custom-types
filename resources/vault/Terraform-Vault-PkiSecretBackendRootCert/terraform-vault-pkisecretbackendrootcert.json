{
    "typeName": "Terraform::Vault::PkiSecretBackendRootCert",
    "description": "Generates a new self-signed CA certificate and private keys for the PKI Secret Backend.\n\n~> **Important** All data provided in the resource configuration will be\nwritten in cleartext to state and plan files generated by Terraform, and\nwill appear in the console output when Terraform runs. Protect these\nartifacts accordingly. See\n[the main provider documentation](../index.html)\nfor more details.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AltNames": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            },
            "description": "List of alternative names."
        },
        "Backend": {
            "type": "string",
            "description": "The PKI secret backend the resource belongs to."
        },
        "Certificate": {
            "type": "string"
        },
        "CommonName": {
            "type": "string",
            "description": "CN of intermediate to create."
        },
        "Country": {
            "type": "string",
            "description": "The country."
        },
        "ExcludeCnFromSans": {
            "type": "boolean",
            "description": "Flag to exclude CN from SANs."
        },
        "Format": {
            "type": "string",
            "description": "The format of data."
        },
        "IpSans": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            },
            "description": "List of alternative IPs."
        },
        "IssuingCa": {
            "type": "string"
        },
        "KeyBits": {
            "type": "number",
            "description": "The number of bits to use."
        },
        "KeyType": {
            "type": "string",
            "description": "The desired key type."
        },
        "Locality": {
            "type": "string",
            "description": "The locality."
        },
        "MaxPathLength": {
            "type": "number",
            "description": "The maximum path length to encode in the generated certificate."
        },
        "Organization": {
            "type": "string",
            "description": "The organization."
        },
        "OtherSans": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            },
            "description": "List of other SANs."
        },
        "Ou": {
            "type": "string",
            "description": "The organization unit."
        },
        "PermittedDnsDomains": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            },
            "description": "List of domains for which certificates are allowed to be issued."
        },
        "PostalCode": {
            "type": "string",
            "description": "The postal code."
        },
        "PrivateKeyFormat": {
            "type": "string",
            "description": "The private key format."
        },
        "Province": {
            "type": "string",
            "description": "The province."
        },
        "Serial": {
            "type": "string"
        },
        "StreetAddress": {
            "type": "string",
            "description": "The street address."
        },
        "Ttl": {
            "type": "string",
            "description": "Time to live."
        },
        "Type": {
            "type": "string",
            "description": "Type of intermediate to create. Must be either \\\"exported\\\" or \\\"internal\\\"."
        },
        "UriSans": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            },
            "description": "List of alternative URIs."
        }
    },
    "additionalProperties": false,
    "required": [
        "Backend",
        "CommonName",
        "Type"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Certificate",
        "/properties/IssuingCa",
        "/properties/Serial"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}