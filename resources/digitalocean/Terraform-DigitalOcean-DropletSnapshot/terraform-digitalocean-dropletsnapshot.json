        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "CreatedAt": {
            "type": "string"
        },
        "DropletId": {
            "type": "string",
            "description": "The ID of the Droplet from which the snapshot will be taken."
        },
        "MinDiskSize": {
            "type": "number"
        },
        "Name": {
            "type": "string",
            "description": "A name for the Droplet snapshot."
        },
        "Regions": {
            "type": "array",
