{
    "typeName": "Terraform::DigitalOcean::SshKey",
    "description": "Provides a DigitalOcean SSH key resource to allow you to manage SSH\nkeys for Droplet access. Keys created with this resource\ncan be referenced in your Droplet configuration via their ID or\nfingerprint.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Fingerprint": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "The name of the SSH key for identification."
        },
        "PublicKey": {
            "type": "string",
            "description": "The public key. If this is a file, it\ncan be read using the file interpolation function."
        }
    },
    "additionalProperties": false,
    "required": [
        "Name",
        "PublicKey"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Fingerprint"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}