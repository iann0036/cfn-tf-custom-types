{
    "typeName": "Terraform::OpenStack::NetworkingPortV2",
    "description": "Manages a V2 port resource within OpenStack.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "AllowedAddressPairs": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "IpAddress": {
                    "type": "string",
                    "description": "The additional IP address."
                },
                "MacAddress": {
                    "type": "string",
                    "description": "The additional MAC address."
                }
            },
            "required": [
                "IpAddress"
            ]
        },
        "Binding": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "HostId": {
                    "type": "string",
                    "description": "The ID of the host to allocate port on."
                },
                "Profile": {
                    "type": "string",
                    "description": "Custom data to be passed as `binding:profile`. Data\nmust be passed as JSON."
                },
                "VnicType": {
                    "type": "string",
                    "description": "VNIC type for the port. Can either be `direct`,\n`direct-physical`, `macvtap`, `normal`, `baremetal` or `virtio-forwarder`.\nDefault value is `normal`."
                }
            },
            "required": []
        },
        "ExtraDhcpOption": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "IpVersion": {
                    "type": "number",
                    "description": "IP protocol version. Defaults to 4."
                },
                "Name": {
                    "type": "string",
                    "description": "Name of the DHCP option."
                },
                "Value": {
                    "type": "string",
                    "description": "Value of the DHCP option."
                }
            },
            "required": [
                "Name",
                "Value"
            ]
        },
        "FixedIp": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "IpAddress": {
                    "type": "string",
                    "description": "IP address desired in the subnet for this port. If\nyou don't specify `ip_address`, an available IP address from the specified\nsubnet will be allocated to this port. This field will not be populated if it\nis left blank or omitted. To retrieve the assigned IP address, use the\n`all_fixed_ips` attribute."
                },
                "SubnetId": {
                    "type": "string",
                    "description": "Subnet in which to allocate IP address for\nthis port."
                }
            },
            "required": [
                "SubnetId"
            ]
        },
        "Timeouts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AdminStateUp": {
            "type": "boolean",
            "description": "Administrative up/down status for the port\n(must be \"true\" or \"false\" if provided). Changing this updates the\n`admin_state_up` of an existing port."
        },
        "AllFixedIps": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "AllSecurityGroupIds": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "AllTags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "Description": {
            "type": "string",
            "description": "Human-readable description of the floating IP. Changing\nthis updates the `description` of an existing port."
        },
        "DeviceId": {
            "type": "string",
            "description": "The ID of the device attached to the port. Changing this\ncreates a new port."
        },
        "DeviceOwner": {
            "type": "string",
            "description": "The device owner of the Port. Changing this creates\na new port."
        },
        "DnsAssignment": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "array",
                "insertionOrder": true,
                "items": {
                    "type": "object",
                    "additionalProperties": false,
                    "properties": {
                        "MapKey": {
                            "type": "string"
                        },
                        "MapValue": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "MapKey",
                        "MapValue"
                    ]
                }
            }
        },
        "DnsName": {
            "type": "string",
            "description": "The port DNS name. Available, when Neutron DNS extension\nis enabled."
        },
        "Id": {
            "type": "string"
        },
        "MacAddress": {
            "type": "string",
            "description": "Specify a specific MAC address for the port. Changing\nthis creates a new port."
        },
        "Name": {
            "type": "string",
            "description": "A unique name for the port. Changing this\nupdates the `name` of an existing port."
        },
        "NetworkId": {
            "type": "string",
            "description": "The ID of the network to attach the port to. Changing\nthis creates a new port."
        },
        "NoFixedIp": {
            "type": "boolean",
            "description": "Create a port with no fixed\nIP address. This will also remove any fixed IPs previously set on a port. `true`\nis the only valid value for this argument."
        },
        "NoSecurityGroups": {
            "type": "boolean",
            "description": "If set to\n`true`, then no security groups are applied to the port. If set to `false` and\nno `security_group_ids` are specified, then the Port will yield to the default\nbehavior of the Networking service, which is to usually apply the \"default\"\nsecurity group."
        },
        "PortSecurityEnabled": {
            "type": "boolean",
            "description": "Whether to explicitly enable or disable\nport security on the port. Port Security is usually enabled by default, so\nomitting argument will usually result in a value of \"true\". Setting this\nexplicitly to `false` will disable port security. In order to disable port\nsecurity, the port must not have any security groups. Valid values are `true`\nand `false`."
        },
        "QosPolicyId": {
            "type": "string",
            "description": "Reference to the associated QoS policy."
        },
        "Region": {
            "type": "string",
            "description": "The region in which to obtain the V2 networking client.\nA networking client is needed to create a port. If omitted, the\n`region` argument of the provider is used. Changing this creates a new\nport."
        },
        "SecurityGroupIds": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "A list\nof security group IDs to apply to the port. The security groups must be\nspecified by ID and not name (as opposed to how they are configured with\nthe Compute Instance)."
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "A set of string tags for the port."
        },
        "TenantId": {
            "type": "string",
            "description": "The owner of the Port. Required if admin wants\nto create a port for another tenant. Changing this creates a new port."
        },
        "ValueSpecs": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "MapKey": {
                        "type": "string"
                    },
                    "MapValue": {
                        "type": "string"
                    }
                },
                "required": [
                    "MapKey",
                    "MapValue"
                ]
            },
            "description": "Map of additional options."
        },
        "AllowedAddressPairs": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/AllowedAddressPairs"
            }
        },
        "Binding": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Binding"
            },
            "maxItems": 1
        },
        "ExtraDhcpOption": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/ExtraDhcpOption"
            }
        },
        "FixedIp": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/FixedIp"
            }
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
        }
    },
    "additionalProperties": false,
    "required": [
        "NetworkId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/AllFixedIps",
        "/properties/AllSecurityGroupIds",
        "/properties/AllTags",
        "/properties/DnsAssignment",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}