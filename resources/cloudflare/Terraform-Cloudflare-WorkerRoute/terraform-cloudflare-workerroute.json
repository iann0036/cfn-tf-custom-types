{
    "typeName": "Terraform::Cloudflare::WorkerRoute",
    "description": "Provides a Cloudflare worker route resource. A route will also require a `cloudflare_worker_script`.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Pattern": {
            "type": "string",
            "description": "The [route pattern](https://developers.cloudflare.com/workers/about/routes/)."
        },
        "ScriptName": {
            "type": "string"
        },
        "ZoneId": {
            "type": "string",
            "description": "The zone ID to add the route to."
        }
    },
    "additionalProperties": false,
    "required": [
        "Pattern",
        "ZoneId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}