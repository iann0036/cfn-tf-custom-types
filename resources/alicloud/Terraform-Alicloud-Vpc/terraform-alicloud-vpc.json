{
    "typeName": "Terraform::Alicloud::Vpc",
    "description": "Provides a VPC resource.\n\n-> **NOTE:** Terraform will auto build a router and a route table while it uses `alicloud_vpc` to build a vpc resource.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "CidrBlock": {
            "type": "string",
            "description": "The CIDR block for the VPC."
        },
        "Description": {
            "type": "string",
            "description": "The VPC description. Defaults to null."
        },
        "Name": {
            "type": "string",
            "description": "The name of the VPC. Defaults to null."
        },
        "ResourceGroupId": {
            "type": "string",
            "description": "The Id of resource group which the VPC belongs."
        },
        "RouteTableId": {
            "type": "string"
        },
        "RouterId": {
            "type": "string"
        },
        "RouterTableId": {
            "type": "string"
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            },
            "description": "A mapping of tags to assign to the resource."
        }
    },
    "additionalProperties": false,
    "required": [
        "CidrBlock"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/RouteTableId",
        "/properties/RouterId",
        "/properties/RouterTableId"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}