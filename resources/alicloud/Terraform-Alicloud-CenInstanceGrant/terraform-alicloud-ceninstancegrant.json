{
    "typeName": "Terraform::Alicloud::CenInstanceGrant",
    "description": "Provides a CEN child instance grant resource, which allow you to authorize a VPC or VBR to a CEN of a different account.\n\nFor more information about how to use it, see [Attach a network in a different account](https://www.alibabacloud.com/help/doc-detail/73645.htm).",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "CenId": {
            "type": "string",
            "description": "The ID of the CEN."
        },
        "CenOwnerId": {
            "type": "string",
            "description": "The owner UID of the  CEN which the child instance granted to."
        },
        "ChildInstanceId": {
            "type": "string",
            "description": "The ID of the child instance to grant."
        }
    },
    "additionalProperties": false,
    "required": [
        "CenId",
        "CenOwnerId",
        "ChildInstanceId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}