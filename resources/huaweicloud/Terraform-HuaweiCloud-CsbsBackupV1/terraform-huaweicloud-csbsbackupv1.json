{
    "typeName": "Terraform::HuaweiCloud::CsbsBackupV1",
    "description": "Provides an HuaweiCloud Backup of Resources.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Timeouts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "BackupName": {
            "type": "string",
            "description": "Name for the backup. The value consists of 1 to 255 characters and can contain only letters, digits, underscores (_), and hyphens (-). Changing backup_name creates a new backup."
        },
        "BackupRecordId": {
            "type": "string"
        },
        "Description": {
            "type": "string",
            "description": "Backup description. The value consists of 0 to 255 characters and must not contain a greater-than sign (>) or less-than sign (<). Changing description creates a new backup."
        },
        "Id": {
            "type": "string"
        },
        "Region": {
            "type": "string"
        },
        "ResourceId": {
            "type": "string",
            "description": "ID of the target to which the backup is restored. Changing this creates a new backup."
        },
        "ResourceType": {
            "type": "string",
            "description": "Type of the target to which the backup is restored. The default value is **OS::Nova::Server** for an ECS. Changing this creates a new backup."
        },
        "Status": {
            "type": "string"
        },
        "VmMetadata": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "CloudServiceType": {
                        "type": "string"
                    },
                    "Disk": {
                        "type": "number"
                    },
                    "Eip": {
                        "type": "string"
                    },
                    "ImageType": {
                        "type": "string"
                    },
                    "Name": {
                        "type": "string"
                    },
                    "PrivateIp": {
                        "type": "string"
                    },
                    "Ram": {
                        "type": "number"
                    },
                    "Vcpus": {
                        "type": "number"
                    }
                }
            }
        },
        "VolumeBackups": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "AverageSpeed": {
                        "type": "number"
                    },
                    "Bootable": {
                        "type": "boolean"
                    },
                    "Id": {
                        "type": "string"
                    },
                    "ImageType": {
                        "type": "string"
                    },
                    "Incremental": {
                        "type": "boolean"
                    },
                    "Name": {
                        "type": "string"
                    },
                    "Size": {
                        "type": "number"
                    },
                    "SnapshotId": {
                        "type": "string"
                    },
                    "SourceVolumeId": {
                        "type": "string"
                    },
                    "SourceVolumeName": {
                        "type": "string"
                    },
                    "SourceVolumeSize": {
                        "type": "number"
                    },
                    "SpaceSavingRatio": {
                        "type": "number"
                    },
                    "Status": {
                        "type": "string"
                    }
                }
            }
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
        }
    },
    "additionalProperties": false,
    "required": [
        "ResourceId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/BackupRecordId",
        "/properties/Id",
        "/properties/Status",
        "/properties/VmMetadata",
        "/properties/VolumeBackups"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}