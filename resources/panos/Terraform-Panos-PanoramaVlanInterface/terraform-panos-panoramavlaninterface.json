{
    "typeName": "Terraform::Panos::PanoramaVlanInterface",
    "description": "This resource allows you to add/update/delete Panorama VLAN interfaces\nfor templates.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AdjustTcpMss": {
            "type": "boolean",
            "description": "Adjust TCP MSS (default: false)."
        },
        "Comment": {
            "type": "string",
            "description": "The interface comment."
        },
        "CreateDhcpDefaultRoute": {
            "type": "boolean",
            "description": "Set to `true` to create a DHCP\ndefault route."
        },
        "DhcpDefaultRouteMetric": {
            "type": "number",
            "description": "The metric for the DHCP default\nroute."
        },
        "EnableDhcp": {
            "type": "boolean",
            "description": "Set to `true` to enable DHCP on this interface."
        },
        "Ipv4MssAdjust": {
            "type": "number",
            "description": "The IPv4 MSS adjust value."
        },
        "Ipv6MssAdjust": {
            "type": "number",
            "description": "The IPv6 MSS adjust value."
        },
        "ManagementProfile": {
            "type": "string",
            "description": "The management profile."
        },
        "Mtu": {
            "type": "number",
            "description": "The MTU."
        },
        "Name": {
            "type": "string",
            "description": "The interface's name.  Must start with `vlan.`."
        },
        "NetflowProfile": {
            "type": "string",
            "description": "The netflow profile."
        },
        "StaticIps": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            },
            "description": "List of static IPv4 addresses to set for this data\ninterface."
        },
        "Template": {
            "type": "string",
            "description": "The template name."
        },
        "Vsys": {
            "type": "string",
            "description": "The vsys that will use this interface (default: `vsys1`)."
        }
    },
    "additionalProperties": false,
    "required": [
        "Name",
        "Template"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}