{
    "typeName": "Terraform::Panos::BgpDampeningProfile",
    "description": "This resource allows you to add/update/delete a BGP dampening profile.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Cutoff": {
            "type": "number",
            "description": "Cutoff threshold value (default: `1.25`)."
        },
        "DecayHalfLifeReachable": {
            "type": "number",
            "description": "Decay half-life while\nreachable, in seconds (default: `300`)."
        },
        "DecayHalfLifeUnreachable": {
            "type": "number",
            "description": "Decay half-life while\nunreachable, in seconds (default: `900`)."
        },
        "Enable": {
            "type": "boolean",
            "description": "Enable or not (default: `true`)."
        },
        "MaxHoldTime": {
            "type": "number",
            "description": "Maximum hold-down time, in\nseconds (default: `900`)."
        },
        "Name": {
            "type": "string",
            "description": "The name."
        },
        "Reuse": {
            "type": "number",
            "description": "Reuse threshold value (default: `0.5`)."
        },
        "VirtualRouter": {
            "type": "string",
            "description": "The virtual router to add this BGP\ndampening profile to."
        }
    },
    "additionalProperties": false,
    "required": [
        "Name",
        "VirtualRouter"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}