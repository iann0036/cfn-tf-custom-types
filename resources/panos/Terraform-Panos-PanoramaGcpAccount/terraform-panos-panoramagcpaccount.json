{
    "typeName": "Terraform::Panos::PanoramaGcpAccount",
    "description": "This resource allows you to add/update/delete GCP accounts on Panorama.\n\nThis resource requires that the GCP plugin be installed.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "CredentialFile": {
            "type": "string",
            "description": "The contents of a GCP credentials file; use the\n`file()` function to pass in the credentials file."
        },
        "CredentialFileEnc": {
            "type": "string"
        },
        "Description": {
            "type": "string",
            "description": "Account description."
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "The account's name."
        },
        "ProjectId": {
            "type": "string",
            "description": "The GCP project ID."
        },
        "ServiceAccountCredentialType": {
            "type": "string",
            "description": "The service account credential\ntype.  Valid values are `gcp` (default) or `gke`."
        }
    },
    "additionalProperties": false,
    "required": [
        "CredentialFile",
        "Name",
        "ProjectId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/CredentialFileEnc",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}