{
    "typeName": "Terraform::Panos::PanoramaIpsecCryptoProfile",
    "description": "This resource allows you to add/update/delete Panorama IPSec crypto profiles\nfor both templates and template stacks.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Authentications": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            },
            "description": "- List of authentication types."
        },
        "DhGroup": {
            "type": "string",
            "description": "The DH group value.  Valid values should start with\nthe string `group`."
        },
        "Encryptions": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            },
            "description": "- List of encryption types.  Valid values\nare `des`, `3des`, `aes-128-cbc`, `aes-192-cbc`, `aes-256-cbc`, `aes-128-gcm`,\n`aes-256-gcm`, and `null`.  Note that the \"gcm\" values are only available in\nPAN-OS 7.0+."
        },
        "Id": {
            "type": "string"
        },
        "LifesizeType": {
            "type": "string",
            "description": "The lifesize type.  Valid values are `kb`, `mb`,\n`gb`, or `tb`."
        },
        "LifesizeValue": {
            "type": "number",
            "description": "the lifesize value."
        },
        "LifetimeType": {
            "type": "string",
            "description": "The lifetime type.  Valid values are `seconds`,\n`minutes`, `hours` (the default), or `days`."
        },
        "LifetimeValue": {
            "type": "number",
            "description": "The lifetime value."
        },
        "Name": {
            "type": "string",
            "description": "The object's name."
        },
        "Protocol": {
            "type": "string",
            "description": "The protocol.  Valid values are `esp` (the default)\nor `ah`."
        },
        "Template": {
            "type": "string",
            "description": "The template name."
        },
        "TemplateStack": {
            "type": "string",
            "description": "The template stack name."
        }
    },
    "additionalProperties": false,
    "required": [
        "Authentications",
        "Encryptions",
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}