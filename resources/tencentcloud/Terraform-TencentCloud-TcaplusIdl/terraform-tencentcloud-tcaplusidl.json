{
    "typeName": "Terraform::TencentCloud::TcaplusIdl",
    "description": "Use this resource to create tcaplus idl file",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AppId": {
            "type": "string",
            "description": "Application id of the idl belongs.."
        },
        "FileContent": {
            "type": "string",
            "description": "Idl file content."
        },
        "FileExtType": {
            "type": "string",
            "description": "File ext type of this idl file. if `file_type` is PROTO  `file_ext_type` must be 'proto',if `file_type` is TDR  `file_ext_type` must be 'xml',if `file_type` is MIX  `file_ext_type` must be 'xml' or 'proto'."
        },
        "FileName": {
            "type": "string",
            "description": "Name of this idl file."
        },
        "FileType": {
            "type": "string",
            "description": "Type of this idl file, Valid values are PROTO,TDR,MIX."
        },
        "Id": {
            "type": "string"
        },
        "TableInfos": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Error": {
                        "type": "string"
                    },
                    "IndexKeySet": {
                        "type": "string"
                    },
                    "KeyFields": {
                        "type": "string"
                    },
                    "SumKeyFieldSize": {
                        "type": "number"
                    },
                    "SumValueFieldSize": {
                        "type": "number"
                    },
                    "TableName": {
                        "type": "string"
                    },
                    "ValueFields": {
                        "type": "string"
                    }
                }
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "AppId",
        "FileContent",
        "FileExtType",
        "FileName",
        "FileType"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id",
        "/properties/TableInfos"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}