{
    "typeName": "Terraform::CloudStack::NetworkAcl",
    "description": "Creates a Network ACL for the given VPC.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Description": {
            "type": "string",
            "description": "The description of the ACL. Changing this forces a\nnew resource to be created."
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "The name of the ACL. Changing this forces a new resource\nto be created."
        },
        "Project": {
            "type": "string",
            "description": "The name or ID of the project to deploy this\ninstance to. Changing this forces a new resource to be created."
        },
        "VpcId": {
            "type": "string",
            "description": "The ID of the VPC to create this ACL for. Changing this\nforces a new resource to be created."
        }
    },
    "additionalProperties": false,
    "required": [
        "Name",
        "VpcId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}