{
    "typeName": "Terraform::OpenTelekomCloud::MaasTaskV1",
    "description": "Manages resource task within OpenTelekomCloud MAAS.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "DstNode": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Ak": {
                    "type": "string",
                    "description": "Specifies the destination bucket Access Key."
                },
                "Bucket": {
                    "type": "string",
                    "description": "Specifies the name of the destination bucket."
                },
                "ObjectKey": {
                    "type": "string",
                    "description": "Specifies the name of the object to be selected in the\ndestination bucket."
                },
                "Region": {
                    "type": "string"
                },
                "Sk": {
                    "type": "string",
                    "description": "Specifies the destination bucket Secret Key."
                }
            },
            "required": [
                "Ak",
                "Bucket",
                "Region",
                "Sk"
            ]
        },
        "SmnInfo": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Language": {
                    "type": "string",
                    "description": "Specifies the management console language used by the\ncurrent users. Users can select en-us."
                },
                "TopicUrn": {
                    "type": "string",
                    "description": "Specifies the SMN message topic URN bound to a migration\ntask."
                },
                "TriggerConditions": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "string"
                    },
                    "description": "Specifies the trigger conditions of sending messages\nusing SMN. The value depending on the state of a migration task. The migration task\nstatus can be SUCCESS or FAIL."
                }
            },
            "required": [
                "TopicUrn",
                "TriggerConditions"
            ]
        },
        "SrcNode": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Ak": {
                    "type": "string",
                    "description": "Specifies the source bucket Access Key."
                },
                "Bucket": {
                    "type": "string",
                    "description": "Specifies the name of the source bucket."
                },
                "CloudType": {
                    "type": "string",
                    "description": "Specifies the source cloud vendor. The default value is\nAWS and only AWS is supported now."
                },
                "ObjectKey": {
                    "type": "string",
                    "description": "Specifies the name of the object to be selected in the\nsource bucket."
                },
                "Region": {
                    "type": "string"
                },
                "Sk": {
                    "type": "string",
                    "description": "Specifies the source bucket Secret Key."
                }
            },
            "required": [
                "Ak",
                "Bucket",
                "ObjectKey",
                "Region",
                "Sk"
            ]
        },
        "Timeouts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Description": {
            "type": "string",
            "description": "Specifies tasks description, which cannot exceed 255\ncharacters. The following special characters are not allowed: <>()\"&."
        },
        "EnableKms": {
            "type": "boolean",
            "description": "Specifies whether to use KMS encryption."
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string"
        },
        "Status": {
            "type": "number"
        },
        "ThreadNum": {
            "type": "number",
            "description": "Specifies the number of threads used by the migration\ntask. The value cannot exceed 50."
        },
        "DstNode": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/DstNode"
            },
            "minItems": 1
        },
        "SmnInfo": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/SmnInfo"
            }
        },
        "SrcNode": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/SrcNode"
            },
            "minItems": 1
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
        }
    },
    "additionalProperties": false,
    "required": [
        "EnableKms",
        "ThreadNum"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id",
        "/properties/Name",
        "/properties/Status"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}