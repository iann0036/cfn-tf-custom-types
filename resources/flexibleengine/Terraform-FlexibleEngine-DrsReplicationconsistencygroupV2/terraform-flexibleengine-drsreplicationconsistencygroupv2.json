{
    "typeName": "Terraform::FlexibleEngine::DrsReplicationconsistencygroupV2",
    "description": "Manages a V2 replicationconsistencygroup resource within FlexibleEngine.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "CreatedAt": {
            "type": "string"
        },
        "Description": {
            "type": "string",
            "description": "The description of the replication consistency group. The description can contain a maximum of 255 bytes."
        },
        "FailureDetail": {
            "type": "string"
        },
        "FaultLevel": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "The name of the replication consistency group. The name can contain a maximum of 255 bytes."
        },
        "PriorityStation": {
            "type": "string",
            "description": "The primary AZ of the replication consistency group. That is the AZ where the production disk belongs."
        },
        "ReplicationIds": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "An array of one or more IDs of the EVS replication pairs used to create the replication consistency group."
        },
        "ReplicationModel": {
            "type": "string",
            "description": "The type of the created replication consistency group. Currently only type hypermetro is supported."
        },
        "ReplicationStatus": {
            "type": "string"
        },
        "Status": {
            "type": "string"
        },
        "UpdatedAt": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "PriorityStation",
        "ReplicationIds"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/CreatedAt",
        "/properties/FailureDetail",
        "/properties/FaultLevel",
        "/properties/Id",
        "/properties/ReplicationStatus",
        "/properties/Status",
        "/properties/UpdatedAt"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}