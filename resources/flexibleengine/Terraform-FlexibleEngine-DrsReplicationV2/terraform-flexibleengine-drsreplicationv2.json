{
    "typeName": "Terraform::FlexibleEngine::DrsReplicationV2",
    "description": "Manages a V2 replication resource within FlexibleEngine.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "CreatedAt": {
            "type": "string"
        },
        "Description": {
            "type": "string",
            "description": "The description of the EVS replication pair. The description can contain a maximum of 255 bytes."
        },
        "FailureDetail": {
            "type": "string"
        },
        "FaultLevel": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "The name of the EVS replication pair. The name can contain a maximum of 255 bytes."
        },
        "PriorityStation": {
            "type": "string",
            "description": "The primary AZ of the EVS replication pair. That is the AZ where the production disk belongs."
        },
        "Progress": {
            "type": "string"
        },
        "RecordMetadata": {
            "type": "string"
        },
        "ReplicationConsistencyGroupId": {
            "type": "string"
        },
        "ReplicationModel": {
            "type": "string",
            "description": "The type of the EVS replication pair. Currently only type hypermetro is supported."
        },
        "ReplicationStatus": {
            "type": "string"
        },
        "Status": {
            "type": "string"
        },
        "UpdatedAt": {
            "type": "string"
        },
        "VolumeIds": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            },
            "description": "An array of one or more IDs of the EVS disks used to create the EVS replication pair."
        }
    },
    "additionalProperties": false,
    "required": [
        "PriorityStation",
        "VolumeIds"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/CreatedAt",
        "/properties/FailureDetail",
        "/properties/FaultLevel",
        "/properties/Id",
        "/properties/Progress",
        "/properties/RecordMetadata",
        "/properties/ReplicationConsistencyGroupId",
        "/properties/ReplicationStatus",
        "/properties/Status",
        "/properties/UpdatedAt"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}