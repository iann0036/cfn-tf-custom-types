{
    "typeName": "Terraform::Bitbucket::Project",
    "description": "This resource allows you to manage your projects in your bitbucket team. \n\n# Example Usage\n\n```hcl\n# Manage your repository\nresource \"bitbucket_project\" \"devops\" {\n  owner = \"my-team\"\n  name  = \"devops\"\n  key = \"DEVOPS\"\n}\n```",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Description": {
            "type": "string",
            "description": "The description of the project."
        },
        "Id": {
            "type": "string"
        },
        "IsPrivate": {
            "type": "boolean",
            "description": "If you want to keep the project private - defaults to true."
        },
        "Key": {
            "type": "string",
            "description": "The key used for this project."
        },
        "Name": {
            "type": "string",
            "description": "The name of the project."
        },
        "Owner": {
            "type": "string",
            "description": "The owner of this project. Can be you or any team you have write access to."
        }
    },
    "additionalProperties": false,
    "required": [
        "Key",
        "Name",
        "Owner"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}