{
    "typeName": "Terraform::AWS::CodestarnotificationsNotificationRule",
    "description": "Provides a CodeStar Notifications Rule.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Target": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Address": {
                    "type": "string",
                    "description": "The ARN of notification rule target. For example, a SNS Topic ARN."
                },
                "Type": {
                    "type": "string",
                    "description": "The type of the notification target. Default value is `SNS`."
                }
            },
            "required": [
                "Address"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Arn": {
            "type": "string"
        },
        "DetailType": {
            "type": "string",
            "description": "The level of detail to include in the notifications for this resource. Possible values are `BASIC` and `FULL`."
        },
        "EventTypeIds": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "A list of event types associated with this notification rule.\nFor list of allowed events see [here](https://docs.aws.amazon.com/codestar-notifications/latest/userguide/concepts.html#concepts-api)."
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "The name of notification rule."
        },
        "Resource": {
            "type": "string",
            "description": "The ARN of the resource to associate with the notification rule."
        },
        "Status": {
            "type": "string",
            "description": "The status of the notification rule. Possible values are `ENABLED` and `DISABLED`, default is `ENABLED`."
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "MapKey": {
                        "type": "string"
                    },
                    "MapValue": {
                        "type": "string"
                    }
                },
                "required": [
                    "MapKey",
                    "MapValue"
                ]
            },
            "description": "A mapping of tags to assign to the resource."
        },
        "Target": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/Target"
            },
            "maxItems": 10
        }
    },
    "additionalProperties": false,
    "required": [
        "DetailType",
        "EventTypeIds",
        "Name",
        "Resource"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Arn",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "*"
            ]
        },
        "read": {
            "permissions": [
                "*"
            ]
        },
        "update": {
            "permissions": [
                "*"
            ]
        },
        "delete": {
            "permissions": [
                "*"
            ]
        },
        "list": {
            "permissions": [
                "*"
            ]
        }
    }
}