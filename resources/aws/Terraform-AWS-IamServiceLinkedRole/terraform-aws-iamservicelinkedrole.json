{
    "typeName": "Terraform::AWS::IamServiceLinkedRole",
    "description": "Provides an [IAM service-linked role](https://docs.aws.amazon.com/IAM/latest/UserGuide/using-service-linked-roles.html).",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Arn": {
            "type": "string"
        },
        "AwsServiceName": {
            "type": "string",
            "description": "The AWS service to which this role is attached. You use a string similar to a URL but without the `http://` in front. For example: `elasticbeanstalk.amazonaws.com`. To find the full list of services that support service-linked roles, check [the docs](https://docs.aws.amazon.com/IAM/latest/UserGuide/reference_aws-services-that-work-with-iam.html)."
        },
        "CreateDate": {
            "type": "string"
        },
        "CustomSuffix": {
            "type": "string",
            "description": "Additional string appended to the role name. Not all AWS services support custom suffixes."
        },
        "Description": {
            "type": "string",
            "description": "The description of the role."
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string"
        },
        "Path": {
            "type": "string"
        },
        "UniqueId": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "AwsServiceName"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Arn",
        "/properties/CreateDate",
        "/properties/Id",
        "/properties/Name",
        "/properties/Path",
        "/properties/UniqueId"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "*"
            ]
        },
        "read": {
            "permissions": [
                "*"
            ]
        },
        "update": {
            "permissions": [
                "*"
            ]
        },
        "delete": {
            "permissions": [
                "*"
            ]
        },
        "list": {
            "permissions": [
                "*"
            ]
        }
    }
}