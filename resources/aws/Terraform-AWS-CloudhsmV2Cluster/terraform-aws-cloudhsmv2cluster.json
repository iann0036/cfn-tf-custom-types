{
    "typeName": "Terraform::AWS::CloudhsmV2Cluster",
    "description": "Creates an Amazon CloudHSM v2 cluster.\n\nFor information about CloudHSM v2, see the\n[AWS CloudHSM User Guide][1] and the [Amazon\nCloudHSM API Reference][2].\n\n~> **NOTE:** CloudHSM can take up to several minutes to be set up.\nPractically no single attribute can be updated except TAGS.\nIf you need to delete a cluster, you have to remove its HSM modules first.\nTo initialize cluster, you have to add an hsm instance to the cluster then sign CSR and upload it.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Timeouts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                },
                "Update": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "ClusterCertificates": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "AwsHardwareCertificate": {
                        "type": "string"
                    },
                    "ClusterCertificate": {
                        "type": "string"
                    },
                    "ClusterCsr": {
                        "type": "string"
                    },
                    "HsmCertificate": {
                        "type": "string"
                    },
                    "ManufacturerHardwareCertificate": {
                        "type": "string"
                    }
                }
            }
        },
        "ClusterId": {
            "type": "string"
        },
        "ClusterState": {
            "type": "string"
        },
        "HsmType": {
            "type": "string",
            "description": "The type of HSM module in the cluster. Currently, only hsm1.medium is supported."
        },
        "Id": {
            "type": "string"
        },
        "SecurityGroupId": {
            "type": "string"
        },
        "SourceBackupIdentifier": {
            "type": "string",
            "description": "The id of Cloud HSM v2 cluster backup to be restored."
        },
        "SubnetIds": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "The IDs of subnets in which cluster will operate."
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "MapKey": {
                        "type": "string"
                    },
                    "MapValue": {
                        "type": "string"
                    }
                },
                "required": [
                    "MapKey",
                    "MapValue"
                ]
            },
            "description": "A mapping of tags to assign to the resource."
        },
        "VpcId": {
            "type": "string"
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
        }
    },
    "additionalProperties": false,
    "required": [
        "HsmType",
        "SubnetIds"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/ClusterCertificates",
        "/properties/ClusterId",
        "/properties/ClusterState",
        "/properties/Id",
        "/properties/SecurityGroupId",
        "/properties/VpcId"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "*"
            ]
        },
        "read": {
