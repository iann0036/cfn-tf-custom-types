{
    "typeName": "Terraform::AWS::SagemakerEndpointConfiguration",
    "description": "Provides a SageMaker endpoint configuration resource.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "ProductionVariants": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "AcceleratorType": {
                    "type": "string"
                },
                "InitialInstanceCount": {
                    "type": "number",
                    "description": "Initial number of instances used for auto-scaling."
                },
                "InitialVariantWeight": {
                    "type": "number"
                },
                "InstanceType": {
                    "type": "string"
                },
                "ModelName": {
                    "type": "string",
                    "description": "The name of the model to use."
                },
                "VariantName": {
                    "type": "string",
                    "description": "The name of the variant. If omitted, Terraform will assign a random, unique name."
                }
            },
            "required": [
                "InitialInstanceCount",
                "InstanceType",
                "ModelName"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Arn": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "KmsKeyArn": {
            "type": "string",
            "description": "Amazon Resource Name (ARN) of a AWS Key Management Service key that Amazon SageMaker uses to encrypt data on the storage volume attached to the ML compute instance that hosts the endpoint."
        },
        "Name": {
            "type": "string",
            "description": "The name of the endpoint configuration. If omitted, Terraform will assign a random, unique name."
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "MapKey": {
                        "type": "string"
                    },
                    "MapValue": {
                        "type": "string"
                    }
                },
                "required": [
                    "MapKey",
                    "MapValue"
                ]
            },
            "description": "A mapping of tags to assign to the resource."
        },
        "ProductionVariants": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/ProductionVariants"
            },
            "minItems": 1
        }
    },
    "additionalProperties": false,
    "required": [],
