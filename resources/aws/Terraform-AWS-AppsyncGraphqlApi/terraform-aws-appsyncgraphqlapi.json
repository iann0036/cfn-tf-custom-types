{
    "typeName": "Terraform::AWS::AppsyncGraphqlApi",
    "description": "Provides an AppSync GraphQL API.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "AdditionalAuthenticationProvider": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "AuthenticationType": {
                    "type": "string"
                },
                "OpenidConnectConfig": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/OpenidConnectConfig"
                    },
                    "maxItems": 1
                },
                "UserPoolConfig": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/UserPoolConfig"
                    },
                    "maxItems": 1
                }
            },
            "required": [
                "AuthenticationType"
            ]
        },
        "LogConfig": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "CloudwatchLogsRoleArn": {
                    "type": "string"
                },
                "FieldLogLevel": {
                    "type": "string"
                }
            },
            "required": [
                "CloudwatchLogsRoleArn",
                "FieldLogLevel"
            ]
        },
        "OpenidConnectConfig": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "AuthTtl": {
                    "type": "number"
                },
                "ClientId": {
                    "type": "string"
                },
                "IatTtl": {
                    "type": "number"
                },
                "Issuer": {
                    "type": "string"
                }
            },
            "required": [
                "Issuer"
            ]
        },
        "UserPoolConfig": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "AppIdClientRegex": {
                    "type": "string"
                },
                "AwsRegion": {
                    "type": "string"
                },
                "UserPoolId": {
                    "type": "string"
                }
            },
            "required": [
                "UserPoolId"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Arn": {
            "type": "string"
        },
        "AuthenticationType": {
            "type": "string",
            "description": "The authentication type. Valid values: `API_KEY`, `AWS_IAM`, `AMAZON_COGNITO_USER_POOLS`, `OPENID_CONNECT`."
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "A user-supplied name for the GraphqlApi."
        },
        "Schema": {
            "type": "string",
            "description": "The schema definition, in GraphQL schema language format. Terraform cannot perform drift detection of this configuration."
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "MapKey": {
                        "type": "string"
                    },
                    "MapValue": {
                        "type": "string"
                    }
                },
                "required": [
                    "MapKey",
                    "MapValue"
                ]
            },
            "description": "A mapping of tags to assign to the resource."
        },
        "Uris": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "MapKey": {
                        "type": "string"
                    },
                    "MapValue": {
                        "type": "string"
                    }
                },
                "required": [
                    "MapKey",
                    "MapValue"
                ]
            }
        },
        "XrayEnabled": {
            "type": "boolean",
            "description": "Whether tracing with X-ray is enabled. Defaults to false."
        },
        "AdditionalAuthenticationProvider": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/AdditionalAuthenticationProvider"
            }
        },
        "LogConfig": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/LogConfig"
            },
            "maxItems": 1
        },
        "OpenidConnectConfig": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/OpenidConnectConfig"
            },
            "maxItems": 1
        },
        "UserPoolConfig": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/UserPoolConfig"
            },
            "maxItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "AuthenticationType",
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Arn",
        "/properties/Id",
        "/properties/Uris"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "*"
            ]
        },
        "read": {
            "permissions": [
                "*"
            ]
        },
        "update": {
            "permissions": [
                "*"
            ]
        },
        "delete": {
            "permissions": [
                "*"
            ]
        },
        "list": {
            "permissions": [
                "*"
            ]
        }
    }
}