                "RoleArn": {
                    "type": "string"
                },
                "UserPoolId": {
                    "type": "string"
                }
            },
            "required": [
                "IdentityPoolId",
                "RoleArn",
                "UserPoolId"
            ]
        },
        "DomainEndpointOptions": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "EnforceHttps": {
                    "type": "boolean"
