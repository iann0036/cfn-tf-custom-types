        "DomainEndpointOptions": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/DomainEndpointOptions"
            },
            "maxItems": 1
        },
        "EbsOptions": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/EbsOptions"
            },
            "maxItems": 1
        },
        "EncryptAtRest": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/EncryptAtRest"
            },
            "maxItems": 1
        },
        "LogPublishingOptions": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/LogPublishingOptions"
            }
        },
        "NodeToNodeEncryption": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/NodeToNodeEncryption"
            },
            "maxItems": 1
        },
        "SnapshotOptions": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/SnapshotOptions"
            },
            "maxItems": 1
        },
        "VpcOptions": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/VpcOptions"
            },
            "maxItems": 1
        },
        "ZoneAwarenessConfig": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/ZoneAwarenessConfig"
            },
            "maxItems": 1
        }
