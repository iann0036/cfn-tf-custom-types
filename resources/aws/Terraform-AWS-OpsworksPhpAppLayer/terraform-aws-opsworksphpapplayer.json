{
    "typeName": "Terraform::AWS::OpsworksPhpAppLayer",
    "description": "Provides an OpsWorks PHP application layer resource.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "EbsVolume": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Encrypted": {
                    "type": "boolean"
                },
                "Iops": {
                    "type": "number",
                    "description": "For PIOPS volumes, the IOPS per disk."
                },
                "MountPoint": {
                    "type": "string",
                    "description": "The path to mount the EBS volume on the layer's instances."
                },
                "NumberOfDisks": {
                    "type": "number",
                    "description": "The number of disks to use for the EBS volume."
                },
                "RaidLevel": {
                    "type": "string",
                    "description": "The RAID level to use for the volume."
                },
                "Size": {
                    "type": "number",
                    "description": "The size of the volume in gigabytes."
                },
                "Type": {
                    "type": "string",
                    "description": "The type of volume to create. This may be `standard` (the default), `io1` or `gp2`."
                }
            },
            "required": [
                "MountPoint",
                "NumberOfDisks",
                "Size"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Arn": {
            "type": "string"
        },
        "AutoAssignElasticIps": {
            "type": "boolean",
            "description": "Whether to automatically assign an elastic IP address to the layer's instances."
        },
        "AutoAssignPublicIps": {
            "type": "boolean",
            "description": "For stacks belonging to a VPC, whether to automatically assign a public IP address to each of the layer's instances."
        },
        "AutoHealing": {
            "type": "boolean",
            "description": "Whether to enable auto-healing for the layer."
        },
        "CustomConfigureRecipes": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "CustomDeployRecipes": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "CustomInstanceProfileArn": {
            "type": "string",
            "description": "The ARN of an IAM profile that will be used for the layer's instances."
        },
        "CustomJson": {
            "type": "string",
            "description": "Custom JSON attributes to apply to the layer."
        },
        "CustomSecurityGroupIds": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "Ids for a set of security groups to apply to the layer's instances."
        },
        "CustomSetupRecipes": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "CustomShutdownRecipes": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "CustomUndeployRecipes": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "DrainElbOnShutdown": {
            "type": "boolean",
            "description": "Whether to enable Elastic Load Balancing connection draining."
        },
        "ElasticLoadBalancer": {
            "type": "string",
            "description": "Name of an Elastic Load Balancer to attach to this layer."
        },
        "Id": {
            "type": "string"
        },
        "InstallUpdatesOnBoot": {
            "type": "boolean",
            "description": "Whether to install OS and package updates on each instance when it boots."
        },
        "InstanceShutdownTimeout": {
            "type": "number",
            "description": "The time, in seconds, that OpsWorks will wait for Chef to complete after triggering the Shutdown event."
        },
        "Name": {
            "type": "string",
            "description": "A human-readable name for the layer."
        },
        "StackId": {
            "type": "string",
            "description": "The id of the stack the layer will belong to."
        },
        "SystemPackages": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "Names of a set of system packages to install on the layer's instances."
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "MapKey": {
                        "type": "string"
                    },
                    "MapValue": {
                        "type": "string"
                    }
                },
                "required": [
                    "MapKey",
                    "MapValue"
                ]
            },
            "description": "A mapping of tags to assign to the resource."
        },
        "UseEbsOptimizedInstances": {
            "type": "boolean",
            "description": "Whether to use EBS-optimized instances."
        },
        "EbsVolume": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/EbsVolume"
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "StackId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Arn",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "*"
            ]
        },
        "read": {
            "permissions": [
                "*"
            ]
        },
        "update": {
            "permissions": [
                "*"
            ]
        },
        "delete": {
            "permissions": [
                "*"
            ]
        },
        "list": {
            "permissions": [
                "*"
            ]
        }
    }
}