{
    "typeName": "Terraform::AWS::CodecommitTrigger",
    "description": "Provides a CodeCommit Trigger Resource.\n\n~> **NOTE on CodeCommit**: The CodeCommit is not yet rolled out\nin all regions - available regions are listed\n[the AWS Docs](https://docs.aws.amazon.com/general/latest/gr/rande.html#codecommit_region).",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Trigger": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Branches": {
                    "type": "array",
                    "insertionOrder": false,
                    "items": {
                        "type": "string"
                    }
                },
                "CustomData": {
                    "type": "string"
                },
                "DestinationArn": {
                    "type": "string"
                },
                "Events": {
                    "type": "array",
                    "insertionOrder": false,
                    "items": {
                        "type": "string"
                    }
                },
                "Name": {
                    "type": "string"
                }
            },
            "required": [
                "DestinationArn",
                "Events",
                "Name"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "ConfigurationId": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "RepositoryName": {
            "type": "string",
            "description": "The name for the repository. This needs to be less than 100 characters."
        },
        "Trigger": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/Trigger"
            },
            "maxItems": 10,
            "minItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "RepositoryName"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/ConfigurationId",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "*"
            ]
        },
        "read": {
            "permissions": [
                "*"
            ]
        },
        "update": {
            "permissions": [
                "*"
            ]
        },
        "delete": {
            "permissions": [
                "*"
            ]
        },
        "list": {
            "permissions": [
                "*"
            ]
        }
    }
}