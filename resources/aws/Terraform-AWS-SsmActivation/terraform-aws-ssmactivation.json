{
    "typeName": "Terraform::AWS::SsmActivation",
    "description": "Registers an on-premises server or virtual machine with Amazon EC2 so that it can be managed using Run Command.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "ActivationCode": {
            "type": "string"
        },
        "Description": {
            "type": "string",
            "description": "The description of the resource that you want to register."
        },
        "ExpirationDate": {
            "type": "string",
            "description": "UTC timestamp in [RFC3339 format](https://tools.ietf.org/html/rfc3339#section-5.8) by which this activation request should expire. The default value is 24 hours from resource creation time. Terraform will only perform drift detection of its value when present in a configuration."
        },
        "Expired": {
            "type": "string"
        },
        "IamRole": {
            "type": "string",
            "description": "The IAM Role to attach to the managed instance."
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "The default name of the registered managed instance."
        },
        "RegistrationCount": {
            "type": "number"
        },
        "RegistrationLimit": {
            "type": "number",
            "description": "The maximum number of managed instances you want to register. The default value is 1 instance."
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "MapKey": {
                        "type": "string"
                    },
                    "MapValue": {
                        "type": "string"
                    }
                },
                "required": [
                    "MapKey",
                    "MapValue"
                ]
            },
            "description": "A mapping of tags to assign to the object."
        }
    },
    "additionalProperties": false,
    "required": [
        "IamRole"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/ActivationCode",
        "/properties/Expired",
        "/properties/Id",
        "/properties/RegistrationCount"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "*"
            ]
        },
        "read": {
            "permissions": [
                "*"
            ]
        },
        "update": {
            "permissions": [
                "*"
            ]
        },
        "delete": {
            "permissions": [
                "*"
            ]
        },
        "list": {
            "permissions": [
                "*"
            ]
        }
    }
}