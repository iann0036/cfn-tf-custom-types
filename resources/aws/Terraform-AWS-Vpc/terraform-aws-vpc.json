{
    "typeName": "Terraform::AWS::Vpc",
    "description": "Provides a VPC resource.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Arn": {
            "type": "string"
        },
        "AssignGeneratedIpv6CidrBlock": {
            "type": "boolean",
            "description": "Requests an Amazon-provided IPv6 CIDR\nblock with a /56 prefix length for the VPC. You cannot specify the range of IP addresses, or\nthe size of the CIDR block. Default is `false`."
        },
        "CidrBlock": {
            "type": "string",
            "description": "The CIDR block for the VPC."
        },
        "DefaultNetworkAclId": {
            "type": "string"
        },
        "DefaultRouteTableId": {
            "type": "string"
        },
        "DefaultSecurityGroupId": {
            "type": "string"
        },
        "DhcpOptionsId": {
            "type": "string"
        },
        "EnableClassiclink": {
            "type": "boolean",
            "description": "A boolean flag to enable/disable ClassicLink\nfor the VPC. Only valid in regions and accounts that support EC2 Classic.\nSee the [ClassicLink documentation][1] for more information. Defaults false."
        },
        "EnableClassiclinkDnsSupport": {
            "type": "boolean",
            "description": "A boolean flag to enable/disable ClassicLink DNS Support for the VPC.\nOnly valid in regions and accounts that support EC2 Classic."
        },
        "EnableDnsHostnames": {
            "type": "boolean",
            "description": "A boolean flag to enable/disable DNS hostnames in the VPC. Defaults false."
        },
        "EnableDnsSupport": {
            "type": "boolean",
            "description": "A boolean flag to enable/disable DNS support in the VPC. Defaults true."
        },
        "Id": {
            "type": "string"
        },
        "InstanceTenancy": {
            "type": "string",
            "description": "A tenancy option for instances launched into the VPC."
        },
        "Ipv6AssociationId": {
            "type": "string"
        },
        "Ipv6CidrBlock": {
            "type": "string"
        },
        "MainRouteTableId": {
            "type": "string"
        },
        "OwnerId": {
            "type": "string"
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "MapKey": {
                        "type": "string"
                    },
                    "MapValue": {
                        "type": "string"
                    }
                },
                "required": [
                    "MapKey",
                    "MapValue"
                ]
            },
            "description": "A mapping of tags to assign to the resource."
        }
    },
    "additionalProperties": false,
    "required": [
        "CidrBlock"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Arn",
        "/properties/DefaultNetworkAclId",
        "/properties/DefaultRouteTableId",
        "/properties/DefaultSecurityGroupId",
        "/properties/DhcpOptionsId",
        "/properties/Id",
        "/properties/Ipv6AssociationId",
        "/properties/Ipv6CidrBlock",
        "/properties/MainRouteTableId",
        "/properties/OwnerId"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "*"
            ]
        },
        "read": {
            "permissions": [
                "*"
            ]
        },
        "update": {
            "permissions": [
                "*"
            ]
        },
        "delete": {
            "permissions": [
                "*"
            ]
        },
        "list": {
            "permissions": [
                "*"
            ]
        }
    }
}