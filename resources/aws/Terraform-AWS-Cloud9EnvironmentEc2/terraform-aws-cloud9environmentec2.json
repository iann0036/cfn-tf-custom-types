{
    "typeName": "Terraform::AWS::Cloud9EnvironmentEc2",
    "description": "Provides a Cloud9 EC2 Development Environment.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Arn": {
            "type": "string"
        },
        "AutomaticStopTimeMinutes": {
            "type": "number",
            "description": "The number of minutes until the running instance is shut down after the environment has last been used."
        },
        "Description": {
            "type": "string",
            "description": "The description of the environment."
        },
        "Id": {
            "type": "string"
        },
        "InstanceType": {
            "type": "string",
            "description": "The type of instance to connect to the environment, e.g. `t2.micro`."
        },
        "Name": {
            "type": "string",
            "description": "The name of the environment."
        },
        "OwnerArn": {
            "type": "string",
            "description": "The ARN of the environment owner. This can be ARN of any AWS IAM principal. Defaults to the environment's creator."
        },
        "SubnetId": {
            "type": "string",
            "description": "The ID of the subnet in Amazon VPC that AWS Cloud9 will use to communicate with the Amazon EC2 instance."
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "MapKey": {
                        "type": "string"
                    },
                    "MapValue": {
                        "type": "string"
                    }
                },
                "required": [
                    "MapKey",
                    "MapValue"
                ]
            },
            "description": "Key-value mapping of resource tags."
        },
        "Type": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "InstanceType",
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Arn",
        "/properties/Id",
        "/properties/Type"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "*"
            ]
        },
        "read": {
            "permissions": [
                "*"
            ]
        },
        "update": {
            "permissions": [
                "*"
            ]
        },
        "delete": {
            "permissions": [
                "*"
            ]
        },
        "list": {
            "permissions": [
                "*"
            ]
        }
    }
}