{
    "typeName": "Terraform::AWS::CodepipelineWebhook",
    "description": "Provides a CodePipeline Webhook.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "AuthenticationConfiguration": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "AllowedIpRange": {
                    "type": "string",
                    "description": "A valid CIDR block for `IP` filtering. Required for `IP`."
                },
                "SecretToken": {
                    "type": "string",
                    "description": "The shared secret for the GitHub repository webhook. Set this as `secret` in your `github_repository_webhook`'s `configuration` block. Required for `GITHUB_HMAC`."
                }
            },
            "required": []
        },
        "Filter": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "JsonPath": {
                    "type": "string",
                    "description": "The [JSON path](https://github.com/json-path/JsonPath) to filter on."
                },
                "MatchEquals": {
                    "type": "string",
                    "description": "The value to match on (e.g. `refs/heads/{Branch}`). See [AWS docs](https://docs.aws.amazon.com/codepipeline/latest/APIReference/API_WebhookFilterRule.html) for details."
                }
            },
            "required": [
                "JsonPath",
                "MatchEquals"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Authentication": {
            "type": "string",
            "description": "The type of authentication  to use. One of `IP`, `GITHUB_HMAC`, or `UNAUTHENTICATED`."
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "The name of the webhook."
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "MapKey": {
                        "type": "string"
                    },
                    "MapValue": {
                        "type": "string"
                    }
                },
                "required": [
                    "MapKey",
                    "MapValue"
                ]
            },
            "description": "A mapping of tags to assign to the resource."
        },
        "TargetAction": {
            "type": "string",
            "description": "The name of the action in a pipeline you want to connect to the webhook. The action must be from the source (first) stage of the pipeline."
        },
        "TargetPipeline": {
            "type": "string",
            "description": "The name of the pipeline."
        },
        "Url": {
            "type": "string"
        },
        "AuthenticationConfiguration": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/AuthenticationConfiguration"
            },
            "maxItems": 1
        },
        "Filter": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/Filter"
            },
            "minItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "Authentication",
        "Name",
        "TargetAction",
        "TargetPipeline"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id",
        "/properties/Url"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "*"
            ]
        },
        "read": {
            "permissions": [
                "*"
            ]
        },
        "update": {
            "permissions": [
                "*"
            ]
        },
        "delete": {
            "permissions": [
                "*"
            ]
        },
        "list": {
            "permissions": [
                "*"
            ]
        }
    }
}