{
    "typeName": "Terraform::AWS::PinpointApp",
    "description": "Provides a Pinpoint App resource.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "CampaignHook": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "LambdaFunctionName": {
                    "type": "string",
                    "description": "Lambda function name or ARN to be called for delivery. Conflicts with `web_url`."
                },
                "Mode": {
                    "type": "string",
                    "description": "What mode Lambda should be invoked in. Valid values for this parameter are `DELIVERY`, `FILTER`."
                },
                "WebUrl": {
                    "type": "string",
                    "description": "Web URL to call for hook. If the URL has authentication specified it will be added as authentication to the request. Conflicts with `lambda_function_name`."
                }
            },
            "required": []
        },
        "Limits": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Daily": {
                    "type": "number",
                    "description": "The maximum number of messages that the campaign can send daily."
                },
                "MaximumDuration": {
                    "type": "number",
                    "description": "The length of time (in seconds) that the campaign can run before it ends and message deliveries stop. This duration begins at the scheduled start time for the campaign. The minimum value is 60."
                },
                "MessagesPerSecond": {
                    "type": "number",
                    "description": "The number of messages that the campaign can send per second. The minimum value is 50, and the maximum is 20000."
                },
                "Total": {
                    "type": "number",
                    "description": "The maximum total number of messages that the campaign can send."
                }
            },
            "required": []
        },
        "QuietTime": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "End": {
                    "type": "string",
                    "description": "The default end time for quiet time in ISO 8601 format. Required if `start` is set."
                },
                "Start": {
                    "type": "string",
                    "description": "The default start time for quiet time in ISO 8601 format. Required if `end` is set."
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "ApplicationId": {
            "type": "string"
        },
        "Arn": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "The application name. By default generated by Terraform."
        },
        "NamePrefix": {
            "type": "string",
            "description": "The name of the Pinpoint application. Conflicts with `name`."
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "MapKey": {
                        "type": "string"
                    },
                    "MapValue": {
                        "type": "string"
                    }
                },
                "required": [
                    "MapKey",
                    "MapValue"
                ]
            },
            "description": "Key-value mapping of resource tags."
        },
        "CampaignHook": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/CampaignHook"
            },
            "maxItems": 1
        },
        "Limits": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Limits"
            },
            "maxItems": 1
        },
        "QuietTime": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/QuietTime"
            },
            "maxItems": 1
        }
    },
    "additionalProperties": false,
    "required": [],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/ApplicationId",
        "/properties/Arn",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "*"
            ]
        },
        "read": {
            "permissions": [
                "*"
            ]
        },
        "update": {
            "permissions": [
                "*"
            ]
        },
        "delete": {
            "permissions": [
                "*"
            ]
        },
        "list": {
            "permissions": [
                "*"
            ]
        }
    }
}