{
    "typeName": "Terraform::OpsGenie::UserContact",
    "description": "Manages a User Contact.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Enabled": {
            "type": "boolean",
            "description": "Enable contact of the user in OpsGenie. Default value is true."
        },
        "Id": {
            "type": "string"
        },
        "Method": {
            "type": "string",
            "description": "This parameter is the contact method of user and should be one of email, sms or voice. Please note that adding mobile is not supported from API."
        },
        "To": {
            "type": "string",
            "description": "to field is the address of given method."
        },
        "Username": {
            "type": "string",
            "description": "The username for contact.(reference)."
        }
    },
    "additionalProperties": false,
    "required": [
        "Method",
        "To",
        "Username"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}