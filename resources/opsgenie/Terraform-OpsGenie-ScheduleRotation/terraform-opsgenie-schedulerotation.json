{
    "typeName": "Terraform::OpsGenie::ScheduleRotation",
    "description": "Manages a Schedule Rotation within Opsgenie.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Participant": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Id": {
                    "type": "string",
                    "description": "The id of the responder."
                },
                "Type": {
                    "type": "string",
                    "description": "The responder type."
                }
            },
            "required": [
                "Type"
            ]
        },
        "TimeRestriction": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Type": {
                    "type": "string",
                    "description": "This parameter should be set time-of-day."
                },
                "Restriction": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/Restriction"
                    }
                },
                "Restrictions": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/Restrictions"
                    }
                }
            },
            "required": [
                "Type"
            ]
        },
        "Restriction": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "EndHour": {
                    "type": "number",
                    "description": "Value of the hour that frame will end.\n* `end_min` - (Required) Value of the minute that frame will end. Minutes may take 0 or 30 as value. Otherwise they will be converted to nearest 0 or 30 automatically."
                },
                "EndMin": {
                    "type": "number",
                    "description": "Value of the minute that frame will end. Minutes may take 0 or 30 as value. Otherwise they will be converted to nearest 0 or 30 automatically."
                },
                "StartHour": {
                    "type": "number",
                    "description": "Value of the hour that frame will start\n* `start_min` - (Required) Value of the minute that frame will start. Minutes may take 0 or 30 as value. Otherwise they will be converted to nearest 0 or 30 automatically.\n* `end_hour` - (Required) Value of the hour that frame will end.\n* `end_min` - (Required) Value of the minute that frame will end. Minutes may take 0 or 30 as value. Otherwise they will be converted to nearest 0 or 30 automatically."
                },
                "StartMin": {
                    "type": "number",
                    "description": "Value of the minute that frame will start. Minutes may take 0 or 30 as value. Otherwise they will be converted to nearest 0 or 30 automatically.\n* `end_hour` - (Required) Value of the hour that frame will end.\n* `end_min` - (Required) Value of the minute that frame will end. Minutes may take 0 or 30 as value. Otherwise they will be converted to nearest 0 or 30 automatically."
                }
            },
            "required": [
                "EndHour",
                "EndMin",
                "StartHour",
                "StartMin"
            ]
        },
        "Restrictions": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "EndDay": {
                    "type": "string"
                },
                "EndHour": {
                    "type": "number"
                },
                "EndMin": {
                    "type": "number"
                },
                "StartDay": {
                    "type": "string"
                },
                "StartHour": {
                    "type": "number"
                },
                "StartMin": {
                    "type": "number"
                }
            },
            "required": [
                "EndDay",
                "EndHour",
                "EndMin",
                "StartDay",
                "StartHour",
                "StartMin"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "EndDate": {
            "type": "string",
            "description": "This parameter takes a date format as (yyyy-MM-dd'T'HH:mm:ssZ) (e.g. 2019-06-11T08:00:00+02:00). Minutes may take 0 or 30 as value. Otherwise they will be converted to nearest 0 or 30 automatically."
        },
        "Id": {
            "type": "string"
        },
        "Length": {
            "type": "number",
            "description": "Length of the rotation with default value 1."
        },
        "Name": {
            "type": "string",
            "description": "Name of rotation."
        },
        "ScheduleId": {
            "type": "string",
            "description": "Identifier of the schedule."
        },
        "StartDate": {
            "type": "string",
            "description": "This parameter takes a date format as (yyyy-MM-dd'T'HH:mm:ssZ) (e.g. 2019-06-11T08:00:00+02:00). Minutes may take 0 or 30 as value. Otherwise they will be converted to nearest 0 or 30 automatically."
        },
        "Type": {
            "type": "string",
            "description": "Type of rotation. May be one of daily, weekly and hourly."
        },
        "Participant": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Participant"
            },
            "minItems": 1
        },
        "TimeRestriction": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/TimeRestriction"
            }
        },
        "Restriction": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Restriction"
            }
        },
        "Restrictions": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Restrictions"
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "ScheduleId",
        "StartDate",
        "Type"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}