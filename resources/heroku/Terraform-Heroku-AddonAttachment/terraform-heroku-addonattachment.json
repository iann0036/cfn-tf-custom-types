{
    "typeName": "Terraform::Heroku::AddonAttachment",
    "description": "Attaches a Heroku Addon Resource to an additional Heroku App.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AddonId": {
            "type": "string",
            "description": "The ID of the existing Heroku Addon to attach."
        },
        "AppId": {
            "type": "string",
            "description": "The ID of the Heroku App to attach to."
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "A friendly name for the Heroku Addon Attachment."
        }
    },
    "additionalProperties": false,
    "required": [
        "AddonId",
        "AppId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}