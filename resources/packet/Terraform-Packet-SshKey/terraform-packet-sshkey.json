{
    "typeName": "Terraform::Packet::SshKey",
    "description": "Provides a resource to manage User SSH keys on your Packet user account. If you create a new device in a project, all the keys of the project's collaborators will be injected to the device.\n\nThe link between User SSH key and device is implicit. If you want to make sure that a key will be copied to a device, you must ensure that the device resource `depends_on` the key resource.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Created": {
            "type": "string"
        },
        "Fingerprint": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "The name of the SSH key for identification."
        },
        "OwnerId": {
            "type": "string"
        },
        "PublicKey": {
            "type": "string",
            "description": "The public key. If this is a file, it\ncan be read using the file interpolation function."
        },
        "Updated": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "Name",
        "PublicKey"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Created",
        "/properties/Fingerprint",
        "/properties/Id",
        "/properties/OwnerId",
        "/properties/Updated"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}