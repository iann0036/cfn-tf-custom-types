{
    "typeName": "Terraform::AzureRM::IothubDps",
    "description": "Manages an IotHub Device Provisioning Service.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "LinkedHub": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "AllocationWeight": {
                    "type": "number",
                    "description": "The weight applied to the IoT Hub. Defaults to 0."
                },
                "ApplyAllocationPolicy": {
                    "type": "boolean"
                },
                "ConnectionString": {
                    "type": "string",
                    "description": "The connection string to connect to the IoT Hub. Changing this forces a new resource."
                },
                "Location": {
                    "type": "string",
                    "description": "The location of the IoT hub. Changing this forces a new resource."
                }
            },
            "required": [
                "ConnectionString",
                "Location"
            ]
        },
        "Sku": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Capacity": {
                    "type": "number",
                    "description": "The number of provisioned IoT Device Provisioning Service units."
                },
                "Name": {
                    "type": "string",
                    "description": "The name of the sku. Possible values are `B1`, `B2`, `B3`, `F1`, `S1`, `S2`, and `S3`."
                }
            },
            "required": [
                "Capacity",
                "Name"
            ]
        },
        "Timeouts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                },
                "Read": {
                    "type": "string"
                },
                "Update": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AllocationPolicy": {
            "type": "string"
        },
        "DeviceProvisioningHostName": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "IdScope": {
            "type": "string"
        },
        "Location": {
            "type": "string",
            "description": "Specifies the supported Azure location where the resource has to be createc. Changing this forces a new resource to be created."
        },
        "Name": {
            "type": "string",
            "description": "Specifies the name of the Iot Device Provisioning Service resource. Changing this forces a new resource to be created."
        },
        "ResourceGroupName": {
            "type": "string",
            "description": "The name of the resource group under which the Iot Device Provisioning Service resource has to be created. Changing this forces a new resource to be created."
        },
        "ServiceOperationsHostName": {
            "type": "string"
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "MapKey": {
                        "type": "string"
                    },
                    "MapValue": {
                        "type": "string"
                    }
                },
                "required": [
                    "MapKey",
                    "MapValue"
                ]
            },
            "description": "A mapping of tags to assign to the resource."
        },
        "LinkedHub": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/LinkedHub"
            }
        },
        "Sku": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/Sku"
            },
            "maxItems": 1,
            "minItems": 1
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
        }
    },
    "additionalProperties": false,
    "required": [
        "Location",
        "Name",
        "ResourceGroupName"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/AllocationPolicy",
        "/properties/DeviceProvisioningHostName",
        "/properties/Id",
        "/properties/IdScope",
        "/properties/ServiceOperationsHostName"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
