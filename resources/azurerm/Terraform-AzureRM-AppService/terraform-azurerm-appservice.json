                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/Microsoft"
                    },
                    "maxItems": 1
                },
                "Twitter": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/Twitter"
                    },
                    "maxItems": 1
                }
            },
            "required": [
                "Enabled"
            ]
        },
        "Backup": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Enabled": {
                    "type": "boolean",
