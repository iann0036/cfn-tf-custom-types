{
    "typeName": "Terraform::OCI::KmsEncryptedData",
    "description": "This resource provides the Encrypted Data resource in Oracle Cloud Infrastructure Kms service.\n\nEncrypts data using the given [EncryptDataDetails](https://docs.cloud.oracle.com/iaas/api/#/en/key/release/datatypes/EncryptDataDetails) resource.\nPlaintext included in the example request is a base64-encoded value of a UTF-8 string.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Timeouts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                },
                "Update": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AssociatedData": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            },
            "description": "Information that can be used to provide an encryption context for the encrypted data. The length of the string representation of the associated data must be fewer than 4096 characters."
        },
        "Ciphertext": {
            "type": "string"
        },
        "CryptoEndpoint": {
            "type": "string",
            "description": "The service endpoint to perform cryptographic operations against. Cryptographic operations include 'Encrypt,' 'Decrypt,' and 'GenerateDataEncryptionKey' operations. see Vault Crypto endpoint."
        },
        "KeyId": {
            "type": "string",
            "description": "The OCID of the key to encrypt with."
        },
        "LoggingContext": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            },
            "description": "Information that provides context for audit logging. You can provide this additional data as key-value pairs to include in the audit logs when audit logging is enabled."
        },
        "Plaintext": {
            "type": "string",
            "description": "The plaintext data to encrypt."
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
        }
    },
    "additionalProperties": false,
    "required": [
        "CryptoEndpoint",
        "KeyId",
        "Plaintext"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Ciphertext"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
