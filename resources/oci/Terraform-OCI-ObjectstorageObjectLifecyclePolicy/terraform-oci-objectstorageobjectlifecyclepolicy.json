{
    "typeName": "Terraform::OCI::ObjectstorageObjectLifecyclePolicy",
    "description": "This resource provides the Object Lifecycle Policy resource in Oracle Cloud Infrastructure Object Storage service.\n\nCreates or replaces the object lifecycle policy for the bucket.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Rules": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Action": {
                    "type": "string"
                },
                "IsEnabled": {
                    "type": "boolean"
                },
                "Name": {
                    "type": "string"
                },
                "TimeAmount": {
                    "type": "string"
                },
                "TimeUnit": {
                    "type": "string"
                },
                "ObjectNameFilter": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/ObjectNameFilter"
                    },
                    "maxItems": 1
                }
            },
            "required": [
                "Action",
                "IsEnabled",
                "Name",
                "TimeAmount",
                "TimeUnit"
            ]
        },
        "Timeouts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                },
                "Update": {
                    "type": "string"
                }
            },
            "required": []
        },
        "ObjectNameFilter": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "ExclusionPatterns": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "string"
                    }
                },
                "InclusionPatterns": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "string"
                    }
                },
                "InclusionPrefixes": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "string"
                    }
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Bucket": {
            "type": "string",
            "description": "The name of the bucket. Avoid entering confidential information. Example: `my-new-bucket1`."
        },
        "Id": {
            "type": "string"
        },
        "Namespace": {
            "type": "string",
            "description": "The Object Storage namespace used for the request."
        },
        "TimeCreated": {
            "type": "string"
        },
        "Rules": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/Rules"
            }
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
        },
        "ObjectNameFilter": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/ObjectNameFilter"
            },
            "maxItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "Bucket",
        "Namespace"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id",
        "/properties/TimeCreated"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}