{
    "typeName": "Terraform::OCI::NosqlTable",
    "description": "This resource provides the Table resource in Oracle Cloud Infrastructure Nosql service.\n\nCreate a new table.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "TableLimits": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "MaxReadUnits": {
                    "type": "number"
                },
                "MaxStorageInGbs": {
                    "type": "number"
                },
                "MaxWriteUnits": {
                    "type": "number"
                }
            },
            "required": [
                "MaxReadUnits",
                "MaxStorageInGbs",
                "MaxWriteUnits"
            ]
        },
        "Timeouts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                },
                "Update": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "CompartmentId": {
            "type": "string",
            "description": "(Updatable) Compartment Identifier."
        },
        "DdlStatement": {
            "type": "string",
            "description": "(Updatable) Complete CREATE TABLE DDL statement."
        },
        "DefinedTags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "MapKey": {
                        "type": "string"
                    },
                    "MapValue": {
                        "type": "string"
                    }
                },
                "required": [
                    "MapKey",
                    "MapValue"
                ]
            },
            "description": "(Updatable) Defined tags for this resource. Each key is predefined and scoped to a namespace.  Example: `{\"foo-namespace\": {\"bar-key\": \"value\"}}`."
        },
        "FreeformTags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "MapKey": {
                        "type": "string"
                    },
                    "MapValue": {
                        "type": "string"
                    }
                },
                "required": [
                    "MapKey",
                    "MapValue"
                ]
            },
            "description": "(Updatable) Simple key-value pair that is applied without any predefined name, type or scope. Exists for cross-compatibility only. Example: `{\"bar-key\": \"value\"}`."
        },
        "Id": {
            "type": "string"
        },
        "LifecycleDetails": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "Table name."
        },
        "Schema": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Columns": {
                        "type": "array",
                        "insertionOrder": false,
                        "items": {
                            "type": "object",
                            "additionalProperties": false,
                            "properties": {
                                "DefaultValue": {
                                    "type": "string"
                                },
                                "IsNullable": {
                                    "type": "boolean"
                                },
                                "Name": {
                                    "type": "string"
                                },
                                "Type": {
                                    "type": "string"
                                }
                            }
                        }
                    },
                    "PrimaryKey": {
                        "type": "array",
                        "insertionOrder": false,
                        "items": {
                            "type": "string"
                        }
                    },
                    "ShardKey": {
                        "type": "array",
                        "insertionOrder": false,
                        "items": {
                            "type": "string"
                        }
                    },
                    "Ttl": {
                        "type": "number"
                    }
                }
            }
        },
        "State": {
            "type": "string"
        },
        "TimeCreated": {
            "type": "string"
        },
        "TimeUpdated": {
            "type": "string"
        },
        "TableLimits": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/TableLimits"
            },
            "maxItems": 1,
            "minItems": 1
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
        }
    },
    "additionalProperties": false,
    "required": [
        "CompartmentId",
        "DdlStatement",
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id",
        "/properties/LifecycleDetails",
        "/properties/Schema",
        "/properties/State",
        "/properties/TimeCreated",
        "/properties/TimeUpdated"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}