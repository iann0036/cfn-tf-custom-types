{
    "typeName": "Terraform::OCI::OsmanagementSoftwareSource",
    "description": "This resource provides the Software Source resource in Oracle Cloud Infrastructure Osmanagement service.\n\nCreates a new custom Software Source on the management system.\nThis will not contain any packages after it is first created,\nand they must be added later.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Timeouts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                },
                "Update": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "ArchType": {
            "type": "string",
            "description": "The architecture type supported by the Software Source."
        },
        "AssociatedManagedInstances": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "DisplayName": {
                        "type": "string"
                    },
                    "Id": {
                        "type": "string"
                    }
                }
            }
        },
        "ChecksumType": {
            "type": "string",
            "description": "(Updatable) The yum repository checksum type used by this software source."
        },
        "CompartmentId": {
            "type": "string",
            "description": "(Updatable) OCID for the Compartment."
        },
        "DefinedTags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            },
            "description": "(Updatable) Defined tags for this resource. Each key is predefined and scoped to a namespace. Example: `{\"foo-namespace.bar-key\": \"value\"}`."
        },
        "Description": {
            "type": "string",
            "description": "(Updatable) Information specified by the user about the software source."
        },
        "DisplayName": {
            "type": "string",
            "description": "(Updatable) User friendly name for the software source."
        },
        "FreeformTags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Key": {
                        "type": "string"
                    },
                    "Value": {
                        "type": "string"
                    }
                },
                "required": [
                    "Key",
                    "Value"
                ]
            },
            "description": "(Updatable) Simple key-value pair that is applied without any predefined name, type or scope. Exists for cross-compatibility only. Example: `{\"bar-key\": \"value\"}`."
        },
        "GpgKeyFingerprint": {
            "type": "string"
        },
        "GpgKeyId": {
            "type": "string"
        },
        "GpgKeyUrl": {
            "type": "string"
        },
        "MaintainerEmail": {
            "type": "string",
            "description": "(Updatable) Email address of the person maintaining this software source."
        },
        "MaintainerName": {
            "type": "string",
            "description": "(Updatable) Name of the person maintaining this software source."
        },
        "MaintainerPhone": {
            "type": "string",
            "description": "(Updatable) Phone number of the person maintaining this software source."
        },
        "Packages": {
            "type": "number"
        },
        "ParentId": {
            "type": "string",
            "description": "OCID for the parent software source, if there is one."
        },
        "ParentName": {
            "type": "string"
        },
        "RepoType": {
            "type": "string"
        },
        "State": {
            "type": "string"
        },
        "Status": {
            "type": "string"
        },
        "Url": {
            "type": "string"
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
        }
    },
    "additionalProperties": false,
    "required": [
        "ArchType",
        "CompartmentId",
        "DisplayName"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/AssociatedManagedInstances",
        "/properties/GpgKeyFingerprint",
        "/properties/GpgKeyId",
        "/properties/GpgKeyUrl",
        "/properties/Packages",
        "/properties/ParentName",
        "/properties/RepoType",
        "/properties/State",
        "/properties/Status",
        "/properties/Url"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}