{
    "typeName": "Terraform::OVH::PubliccloudPrivateNetworkSubnet",
    "description": "~> __DEPRECATED:__ Use [`ovh_cloud_network_private_subnet`](./cloud_network_private_subnet.html) instead.\n\nCreates a subnet in a private network of a public cloud project.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Cidr": {
            "type": "string"
        },
        "Dhcp": {
            "type": "boolean",
            "description": "Enable DHCP.\nChanging this forces a new resource to be created. Defaults to false.\n_."
        },
        "End": {
            "type": "string",
            "description": "Last ip for this region.\nChanging this value recreates the subnet."
        },
        "GatewayIp": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "IpPools": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "Dhcp": {
                        "type": "boolean"
                    },
                    "End": {
                        "type": "string"
                    },
                    "Network": {
                        "type": "string"
                    },
                    "Region": {
                        "type": "string"
                    },
                    "Start": {
                        "type": "string"
                    }
                }
            }
        },
        "Network": {
            "type": "string",
            "description": "Global network in CIDR format.\nChanging this value recreates the subnet."
        },
        "NetworkId": {
            "type": "string",
            "description": "The id of the network.\nChanging this forces a new resource to be created."
        },
        "NoGateway": {
            "type": "boolean",
            "description": "Set to true if you don't want to set a default gateway IP.\nChanging this value recreates the resource. Defaults to false."
        },
        "ProjectId": {
            "type": "string",
            "description": "The id of the public cloud project. If omitted,\nthe `OVH_PROJECT_ID` environment variable is used.\nChanging this forces a new resource to be created."
        },
        "Region": {
            "type": "string",
            "description": "The region in which the network subnet will be created.\nEx.: \"GRA1\". Changing this value recreates the resource."
        },
        "Start": {
            "type": "string",
            "description": "First ip for this region.\nChanging this value recreates the subnet."
        }
    },
    "additionalProperties": false,
    "required": [
        "End",
        "Network",
        "NetworkId",
        "Region",
        "Start"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Cidr",
        "/properties/GatewayIp",
        "/properties/Id",
        "/properties/IpPools"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}