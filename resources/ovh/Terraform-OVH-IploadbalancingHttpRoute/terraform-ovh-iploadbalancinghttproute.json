{
    "typeName": "Terraform::OVH::IploadbalancingHttpRoute",
    "description": "Manage http route for a loadbalancer service",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "Action": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Status": {
                    "type": "number"
                },
                "Target": {
                    "type": "string"
                },
                "Type": {
                    "type": "string"
                }
            },
            "required": [
                "Type"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "DisplayName": {
            "type": "string",
            "description": "Human readable name for your route, this field is for you."
        },
        "FrontendId": {
            "type": "number",
            "description": "Route traffic for this frontend."
        },
        "Id": {
            "type": "string"
        },
        "ServiceName": {
            "type": "string",
            "description": "The internal name of your IP load balancing."
        },
        "Weight": {
            "type": "number",
            "description": "Route priority ([0..255]). 0 if null. Highest priority routes are evaluated first. Only the first matching route will trigger an action."
        },
        "Action": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/Action"
            },
            "minItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "ServiceName"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}