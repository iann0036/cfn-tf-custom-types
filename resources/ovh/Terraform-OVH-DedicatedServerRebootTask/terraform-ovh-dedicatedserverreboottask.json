{
    "typeName": "Terraform::OVH::DedicatedServerRebootTask",
    "description": "Reboot your Dedicated Server.\n\n~> __WARNING__: After some delay, if the task is marked as `done`, the Provider\nmay purge it. To avoid raising errors when terraform refreshes its plan, \n404 errors are ignored on Resource Read, thus some information may be lost\nafter a while.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Comment": {
            "type": "string"
        },
        "DoneDate": {
            "type": "string"
        },
        "Function": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Keepers": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            },
            "description": "List of values traccked to trigger reboot, used also to form implicit dependencies."
        },
        "LastUpdate": {
            "type": "string"
        },
        "ServiceName": {
            "type": "string",
            "description": "The service_name of your dedicated server."
        },
        "StartDate": {
            "type": "string"
        },
        "Status": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "Keepers",
        "ServiceName"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Comment",
        "/properties/DoneDate",
        "/properties/Function",
        "/properties/Id",
        "/properties/LastUpdate",
        "/properties/StartDate",
        "/properties/Status"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}