{
    "typeName": "Terraform::Gitlab::DeployKey",
    "description": "This resource allows you to create and manage deploy keys for your GitLab projects.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "CanPush": {
            "type": "boolean",
            "description": "Allow this deploy key to be used to push changes to the project.  Defaults to `false`. **NOTE::** this cannot currently be managed."
        },
        "Id": {
            "type": "string"
        },
        "Key": {
            "type": "string",
            "description": "The public ssh key body."
        },
        "Project": {
            "type": "string",
            "description": "The name or id of the project to add the deploy key to."
        },
        "Title": {
            "type": "string",
            "description": "A title to describe the deploy key with."
        }
    },
    "additionalProperties": false,
    "required": [
        "Key",
        "Project",
        "Title"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}