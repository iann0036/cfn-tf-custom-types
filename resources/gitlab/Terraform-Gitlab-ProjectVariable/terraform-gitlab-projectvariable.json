{
    "typeName": "Terraform::Gitlab::ProjectVariable",
    "description": "This resource allows you to create and manage CI/CD variables for your GitLab projects.\nFor further information on variables, consult the [gitlab\ndocumentation](https://docs.gitlab.com/ce/ci/variables/README.html#variables).",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "EnvironmentScope": {
            "type": "string",
            "description": "The environment_scope of the variable."
        },
        "Id": {
            "type": "string"
        },
        "Key": {
            "type": "string",
            "description": "The name of the variable."
        },
        "Masked": {
            "type": "boolean",
            "description": "If set to `true`, the variable will be masked if it would have been written to the logs. Defaults to `false`."
        },
        "Project": {
            "type": "string",
            "description": "The name or id of the project to add the hook to."
        },
        "Protected": {
            "type": "boolean",
            "description": "If set to `true`, the variable will be passed only to pipelines running on protected branches and tags. Defaults to `false`."
        },
        "Value": {
            "type": "string",
            "description": "The value of the variable."
        },
        "VariableType": {
            "type": "string",
            "description": "The type of a variable. Available types are: env_var (default) and file."
        }
    },
    "additionalProperties": false,
    "required": [
        "Key",
        "Project",
        "Value"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}