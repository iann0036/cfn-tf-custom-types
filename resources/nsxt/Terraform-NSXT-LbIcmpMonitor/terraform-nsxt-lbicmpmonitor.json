        "Revision": {
            "type": "number"
        },
        "RiseCount": {
            "type": "number",
            "description": "Number of consecutive checks must pass before marking it up."
        },
        "Timeout": {
            "type": "number",
            "description": "Number of seconds the target has in which to respond to the monitor request."
        },
        "Tag": {
