{
    "typeName": "Terraform::OPC::ComputeVpnEndpointV2",
    "description": "The ``opc_compute_vpn_endpoint_v2`` resource creates and manages an VPN Endpoint V2 in an Oracle Cloud Infrastructure Compute Classic identity domain.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "PhaseOneSettings": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "DhGroup": {
                    "type": "string"
                },
                "Encryption": {
                    "type": "string"
                },
                "Hash": {
                    "type": "string"
                },
                "Lifetime": {
                    "type": "number"
                }
            },
            "required": [
                "DhGroup",
                "Encryption",
                "Hash"
            ]
        },
        "PhaseTwoSettings": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Encryption": {
                    "type": "string"
                },
                "Hash": {
                    "type": "string"
                },
                "Lifetime": {
                    "type": "number"
                }
            },
            "required": [
                "Encryption",
                "Hash"
            ]
        },
        "Timeouts": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                },
                "Update": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "CustomerVpnGateway": {
            "type": "string",
            "description": "The ip address of the VPN gateway in your data center through which you want to connect to the Oracle Cloud VPN gateway."
        },
        "Description": {
            "type": "string",
            "description": "A description of the VPN Endpoint V2."
        },
        "Enabled": {
            "type": "boolean",
            "description": "Enables or disables the VPN Endpoint V2. Set to true by default."
        },
        "IkeIdentifier": {
            "type": "string",
            "description": "The Internet Key Exchange (IKE) ID. If you don't specify a value, the default value is the public IP address of the cloud gateway."
        },
        "IpNetwork": {
            "type": "string",
            "description": "The name of the IP network on which the cloud gateway is created by VPNaaS."
        },
        "LocalGatewayIpAddress": {
            "type": "string",
            "description": "Public IP Address of the Local Gateway."
        },
        "LocalGatewayPrivateIpAddress": {
            "type": "string",
            "description": "Private IP Address of the Local Gateway."
        },
        "Name": {
            "type": "string",
            "description": "The name of the VPN Endpoint V2."
        },
        "PreSharedKey": {
            "type": "string",
            "description": "The pre-shared VPN key."
        },
        "ReachableRoutes": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            },
            "description": "A list of routes (CIDR prefixes) that are reachable through this VPN tunnel."
        },
        "RequirePerfectForwardSecrecy": {
            "type": "boolean",
            "description": "Boolean specifying whether Perfect Forward Secrecy is enabled. Set to true by default."
        },
        "Tags": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            },
            "description": "List of tags that may be applied to the VPN Endpoint V2."
        },
        "TunnelStatus": {
            "type": "string"
        },
        "Uri": {
            "type": "string",
            "description": "The Uniform Resource Identifier for the VPN Endpoint V2."
        },
        "VnicSets": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            },
            "description": "A list of vnic sets that traffics is allowed to and from."
        },
        "PhaseOneSettings": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/PhaseOneSettings"
            },
            "maxItems": 1
        },
        "PhaseTwoSettings": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/PhaseTwoSettings"
            },
            "maxItems": 1
        },
        "Timeouts": {
            "$ref": "#/definitions/Timeouts"
        }
    },
    "additionalProperties": false,
    "required": [
        "CustomerVpnGateway",
        "IpNetwork",
        "Name",
        "PreSharedKey",
        "ReachableRoutes",
        "VnicSets"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/LocalGatewayIpAddress",
        "/properties/LocalGatewayPrivateIpAddress",
        "/properties/TunnelStatus",
        "/properties/Uri"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}