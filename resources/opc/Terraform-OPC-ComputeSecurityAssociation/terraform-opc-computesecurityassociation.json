{
    "typeName": "Terraform::OPC::ComputeSecurityAssociation",
    "description": "The ``opc_compute_security_association`` resource creates and manages an association between an instance and a security\nlist in an Oracle Cloud Infrastructure Compute Classic identity domain.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "The Name for the Security Association. If not specified, one is created automatically. Changing this forces a new resource to be created."
        },
        "Seclist": {
            "type": "string",
            "description": "The name of the security list to associate the instance to."
        },
        "Vcable": {
            "type": "string",
            "description": "The `vcable` of the instance to associate to the security list."
        }
    },
    "additionalProperties": false,
    "required": [
