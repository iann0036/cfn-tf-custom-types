{
    "typeName": "Terraform::OPC::ComputeIpNetwork",
    "description": "The ``opc_compute_ip_network`` resource creates and manages an IP Network in an Oracle Cloud Infrastructure Compute Classic identity domain.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Description": {
            "type": "string",
            "description": "The description of the IP Network."
        },
        "Id": {
            "type": "string"
        },
        "IpAddressPrefix": {
            "type": "string",
            "description": "The IPv4 address prefix, in CIDR format."
        },
        "IpNetworkExchange": {
            "type": "string",
            "description": "Specify the IP Network exchange to which the IP Network belongs to."
        },
        "Name": {
            "type": "string",
            "description": "The name of the IP Network. Changing this name forces a new resource to be created."
        },
        "PublicNaptEnabled": {
            "type": "boolean",
            "description": "If true, enable public internet access using NAPT for VNICs without any public IP Reservation. Defaults to `false`."
        },
        "Tags": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "Uri": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "IpAddressPrefix",
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id",
        "/properties/Uri"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}