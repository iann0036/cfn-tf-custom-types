{
    "typeName": "Terraform::Google::SqlSslCert",
    "description": "Creates a new Google SQL SSL Cert on a Google SQL Instance. For more information, see the [official documentation](https://cloud.google.com/sql/), or the [JSON API](https://cloud.google.com/sql/docs/mysql/admin-api/v1beta4/sslCerts).\n\n~> **Note:** All arguments including the private key will be stored in the raw state as plain-text.\n[Read more about sensitive data in state](/docs/state/sensitive-data.html).",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Cert": {
            "type": "string"
        },
        "CertSerialNumber": {
            "type": "string"
        },
        "CommonName": {
            "type": "string",
            "description": "The common name to be used in the certificate to identify the\nclient. Constrained to [a-zA-Z.-_ ]+. Changing this forces a new resource to be created."
        },
        "CreateTime": {
            "type": "string"
        },
        "ExpirationTime": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Instance": {
            "type": "string",
            "description": "The name of the Cloud SQL instance. Changing this\nforces a new resource to be created."
        },
        "PrivateKey": {
            "type": "string"
        },
        "Project": {
            "type": "string",
            "description": "The ID of the project in which the resource belongs. If it\nis not provided, the provider project is used."
        },
        "ServerCaCert": {
            "type": "string"
        },
        "Sha1Fingerprint": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "CommonName",
        "Instance"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Cert",
        "/properties/CertSerialNumber",
        "/properties/CreateTime",
        "/properties/ExpirationTime",
        "/properties/Id",
        "/properties/PrivateKey",
        "/properties/ServerCaCert",
        "/properties/Sha1Fingerprint"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}