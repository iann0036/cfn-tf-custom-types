{
    "typeName": "Terraform::Circonus::Worksheet",
    "description": "The ``circonus_worksheet`` resource creates and manages a\n[Circonus Worksheet](https://login.circonus.com/resources/api/calls/worksheet).",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {
        "SmartQueries": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Name": {
                    "type": "string"
                },
                "Order": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "string"
                    }
                },
                "Query": {
                    "type": "string"
                }
            },
            "required": [
                "Name",
                "Query"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Description": {
            "type": "string",
            "description": "Description of what the worksheet is for."
        },
        "Favourite": {
            "type": "boolean",
            "description": "Mark (star) this worksheet as a favorite. Default is `false`."
        },
        "Graphs": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "A list of graphs that compose this worksheet."
        },
        "Id": {
            "type": "string"
        },
        "Notes": {
            "type": "string",
            "description": "A place to store notes about this worksheet."
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "A list of tags assigned to this worksheet."
        },
        "Title": {
            "type": "string",
            "description": "The title of the worksheet."
        },
        "SmartQueries": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/SmartQueries"
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "Title"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}