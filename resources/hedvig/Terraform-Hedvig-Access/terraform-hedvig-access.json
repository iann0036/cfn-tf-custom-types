{
    "typeName": "Terraform::Hedvig::Access",
    "description": "A Hedvig Access adds an address to an ACL of a controller of a vdisk. This allows for management of access resources.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Address": {
            "type": "string",
            "description": "The actual address that this Access is providing access to."
        },
        "Host": {
            "type": "string",
            "description": "The fully qualified domain name of the controller this Access is associated with."
        },
        "Id": {
            "type": "string"
        },
        "Type": {
            "type": "string",
            "description": "The type of address provided in `address`. Can be `host`, `ip` or `iqn`."
        },
        "Vdisk": {
            "type": "string",
            "description": "The name of the Vdisk that this Access is associated with."
        }
    },
    "additionalProperties": false,
    "required": [
        "Address",
        "Host",
        "Type",
        "Vdisk"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "read": {
            "permissions": []
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:PutObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "secretsmanager:GetSecretValue"
            ]
        },
        "list": {
            "permissions": []
        }
    }
}